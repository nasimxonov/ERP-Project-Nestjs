generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           String    @id
  first_name   String
  last_name    String
  username     String    @unique
  password     String
  email        String?   @unique
  googleId     String?   @unique
  phone_number String
  birth_date   DateTime
  image        String?
  sex          Sex

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("user")
}

enum Sex {
  Male
  Female
}

